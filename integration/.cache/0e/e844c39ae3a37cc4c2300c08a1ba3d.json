   font-family: \"Fondamento-Regular\";\n    src: url(\"~/fonts/Fondamento-Regular.ttf\");\n}\n\n@font-face{\n    font-family: \"Fondamento-Italic\";\n    src: url(\"~/fonts/Fondamento-Italic.ttf\");\n}\n\n@font-face{\n    font-family: \"Josefin-Regular\";\n    src: url(\"~/fonts/JosefinSans-Regular.ttf\");\n}\n\n@font-face{\n    font-family: \"Josefin-Bold\";\n    src: url(\"~/fonts/JosefinSans-Bold.ttf\");\n}\n\n@font-face{\n    font-family: \"Josefin-MediumItalic\";\n    src: url(\"~/fonts/JosefinSans-MediumItalic.ttf\");\n}\n\n@font-face{\n    font-family: \"Josefin-Medium\";\n    src: url(\"~/fonts/JosefinSans-Medium.ttf\");\n}","scss/layout/_global.scss":"body{\n    background-color: $background-color;\n    color: $main-text-color;\n    font-family: $font-main;\n    width: 100%;\n    height: 100%;\n    overflow-x: hidden;\n}","scss/abstract/_variables.scss":"// Colors\n$background-color: $black;\n$main-text-color: $sand;\n$hover-color: $light-sand;\n$background-main: $black;\n$header-font-color: $sand;\n\n// Fonts\n$font-title: \"Fondamento-Regular\";\n$font-main: \"Fondamento-Regular\";\n$font-footer: \"Josefin-Medium\";","scss/layout/_header.scss":".header{\n    margin-top: 25px;\n    margin-bottom: 6rem;\n    background-color: $background-color;\n    display: flex;\n    flex-direction: column;\n    justify-content: space-evenly;\n\n    .logo{\n        width: 100%;\n        display: flex;\n        justify-content: space-around;\n\n        &__image1{\n            margin: auto 0 auto 0;\n            width: 20em;\n            transform: rotateY(180deg);\n            filter: invert(55%) sepia(88%) saturate(281%) hue-rotate(4deg) brightness(89%) contrast(85%);\n        }\n\n        &__image2{\n            margin: auto 0 auto 0;\n            width: 20em;\n            filter: invert(55%) sepia(88%) saturate(281%) hue-rotate(4deg) brightness(89%) contrast(85%);\n        }\n\n        &__title{\n            margin: auto 0 auto 0;\n            font-family: $font-title;\n            font-size: 6.5rem;\n            // text-transform: uppercase;\n        }\n\n        &__menu{\n            margin: auto;\n            font-size: 35px;\n\n            &__list{\n                display: flex;\n                justify-content: center;\n\n                &__item{\n                    padding: 15px;\n\n                    &__link{\n                        text-decoration: none;\n                        color: $main-text-color;\n                        font-family: $font-main;\n                    }\n\n                    &__link:hover{\n                        color: $hover-color;\n                    }\n                }\n            }\n        }\n    }\n}","scss/abstract/_colors.scss":"$white: #d9d9d9;\n$black: #272727;\n$sand: #b29148;\n$light-sand: #d0bc90;","scss/layout/_gallery.scss":".banner{\n    width: 75%;\n    margin: auto;\n    display: flex;\n    justify-content: center;\n\n    &__title{\n        margin: 35px;\n        font-size: 50px;\n        font-family: $font-title;\n        text-transform: uppercase;\n    }\n}\n\n.gallery{\n    width: 85%;\n    margin: auto;\n\n    &__container{\n        display: grid;\n        grid-template-columns: repeat(12, 1fr);\n        grid-template-rows: repeat(12, 5vw);\n        grid-gap: 15px;\n\n        &__item{\n            width: auto;\n            img{\n                width: 100%;\n                height: 100%;\n                object-fit: cover;\n            }\n        }\n\n        .item-1{\n            grid-column: 1 / 4;\n            grid-row: 1 / 7;\n        }\n\n        .item-2{\n            grid-column: 4 / 7;\n            grid-row: 1 / 5;\n        }\n\n        .item-3{\n            grid-column: 7 / 10;\n            grid-row: 1 / 5;\n        }\n\n        .item-4{\n            grid-column: 10 / 13;\n            grid-row: 1 / 7;\n        }\n\n        .item-5{\n            grid-column: 4 / 10;\n            grid-row: 5 / 9;\n        }\n\n        .item-6{\n            grid-column: 4 / 7;\n            grid-row: 9 / 13;\n        }\n\n        .item-7{\n            grid-column: 7 / 10;\n            grid-row: 9 / 13;\n        }\n\n        .item-8{\n            grid-column: 1 / 4;\n            grid-row: 7 / 13;\n        }\n\n        .item-9{\n            grid-column: 10 / 13;\n            grid-row: 7 / 13;\n        }\n    }\n}"},"lineCount":null}},"error":null,"hash":"947742eb11d470ffa81bc2b116a78433","cacheData":{"env":{}}}