: #b29148;","scss/abstract/_variables.scss":"// Colors\n$background-color: $white;\n$main-text-color: $black;\n$hover-color: $sand;\n$background-main: $black;\n$header-font-color: $sand;\n\n// Fonts\n$font-title: \"Crimson-Text-Bold\";\n$font-main: \"Crimson-Text\"","scss/layout/_header.scss":".header{\n    margin-top: 25px;\n    background-color: $background-color;\n    display: flex;\n    justify-content: space-evenly;\n\n    .logo{\n        width: 100%;\n        display: flex;\n        justify-content: space-around;\n\n        &__image{\n            margin: auto 0 auto 0;\n            width: 15em;\n        }\n\n        &__title{\n            margin: auto 0 auto 0;\n            font-family: $font-title;\n            font-size: 70px;\n            text-transform: uppercase;\n        }\n\n        &__menu{\n            margin: auto 0 auto 0;\n            font-size: 35px;\n\n            &__list{\n                display: flex;\n                justify-content: space-between;\n\n                &__item{\n                    margin-left: 25px;\n\n                    &__link{\n                        text-decoration: none;\n                        color: $main-text-color;\n                    }\n\n                    &__link:hover{\n                        color: $hover-color;\n                    }\n                }\n            }\n        }\n    }\n}","scss/layout/_gallery.scss":".banner{\n    width: 75%;\n    margin: auto;\n    text-align: center;\n\n    &__title{\n        margin: 35px;\n        font-size: 50px;\n        text-transform: uppercase;\n    }\n}\n\n.gallery{\n    width: 85%;\n    margin: auto;\n\n    &__container{\n        display: grid;\n        grid-template-columns: repeat(12, 1fr);\n        grid-template-rows: repeat(12, 5vw);\n        grid-gap: 15px;\n\n        &__item{\n            width: auto;\n            img{\n                width: 100%;\n                height: 100%;\n                object-fit: cover;\n            }\n        }\n\n        .item-1{\n            grid-column: 1 / 4;\n            grid-row: 1 / 7;\n        }\n\n        .item-2{\n            grid-column: 4 / 7;\n            grid-row: 1 / 5;\n        }\n\n        .item-3{\n            grid-column: 7 / 10;\n            grid-row: 1 / 5;\n        }\n\n        .item-4{\n            grid-column: 10 / 13;\n            grid-row: 1 / 7;\n        }\n\n        .item-5{\n            grid-column: 4 / 10;\n            grid-row: 5 / 9;\n        }\n\n        .item-6{\n            grid-column: 4 / 7;\n            grid-row: 9 / 13;\n        }\n\n        .item-7{\n            grid-column: 7 / 10;\n            grid-row: 9 / 13;\n        }\n\n        .item-8{\n            grid-column: 1 / 4;\n            grid-row: 7 / 13;\n        }\n\n        .item-9{\n            grid-column: 10 / 13;\n            grid-row: 7 / 13;\n        }\n    }\n}"},"lineCount":null}},"error":null,"hash":"f38f065d218bf9f0cab09905a479dfb3","cacheData":{"env":{}}}