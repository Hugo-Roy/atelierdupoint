.contact {
  width: 75%;
  margin: auto;
  text-align: center;
  font-family: "Josefin-Medium";
  font-size: 1.5em;
}
@media (max-width: 599px) {
  .contact {
    font-size: 1.2em;
  }
}
.contact p {
  color: #d9d9d9;
}
.footer {
  margin: 3em auto;
  width: 55%;
  display: flex;
  flex-direction: column;
}
.footer__disclaimer {
  font-family: "Josefin-Medium";
  text-align: center;
  color: #d9d9d9;
}
.footer__social {
  display: flex;
  justify-content: center;
}
.footer__social__link {
  color: #d9d9d9;
  font-size: 50px;
  padding: 25px;
  transition: all 0.4s ease-in-out;
}
.footer__social__link:hover {
  color: #BEA060;
}
.error {
  width: 75%;
  margin: auto;
  text-align: center;
}
@media (max-width: 599px) {
  .error {
    width: 95%;
  }
}
@media (min-width: 600px) and (max-width: 899px) {
  .error {
    width: 95%;
  }
}
@media (min-width: 900px) {
  .error {
    width: 95%;
  }
}
.error__title {
  margin: 35px;
  font-size: 4em;
  text-transform: lowercase;
  letter-spacing: 2px;
  font-weight: lighter;
  font-family: "Germinabunt";
  color: #d9d9d9;
}
@media (max-width: 599px) {
  .error__title {
    padding-top: 1em;
    font-size: 2.3em;
  }
}