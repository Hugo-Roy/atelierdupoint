* {
  box-sizing: border-box;
}
body {
  background-image: url("e78cf486096c5e11af4698968bc34ad5") !important;
  background-size: cover !important;
  background-position-y: center !important;
  background-attachment: fixed !important;
  background-color: #1b0101 !important;
  color: #d9d9d9;
  font-family: "Germinabunt";
  width: 100%;
  height: 100%;
  overflow-x: hidden;
  margin: auto;
}
.header {
  margin-top: 25px;
  margin-bottom: 6rem;
  display: flex;
  flex-direction: column;
  justify-content: space-evenly;
}
@media (max-width: 599px) {
  .header {
    margin-bottom: 0;
  }
}
@media (min-width: 600px) and (max-width: 899px) {
  .header {
    margin-bottom: 2em;
  }
}
@media (min-width: 900px) {
  .header {
    margin-bottom: 4em;
  }
}
.header .logo {
  width: 100%;
  display: flex;
  justify-content: space-around;
  transition: all 0.6s ease-in;
}
@media (min-width: 900px) {
  .header .logo {
    text-align: center;
  }
}
@media (max-width: 599px) {
  .header .logo {
    text-align: center;
  }
}
.header .logo__image1 {
  margin: auto 0 auto 0;
  width: 20em;
  transform: rotateY(180deg);
  filter: invert(0.8);
}
@media (max-width: 599px) {
  .header .logo__image1 {
    width: 10em;
  }
}
@media (min-width: 900px) {
  .header .logo__image2 {
    margin: auto 0 auto 0;
    width: 20em;
    filter: invert(0.8);
    display: initial;
  }
}
@media (min-width: 600px) and (max-width: 899px) {
  .header .logo__image2 {
    display: none;
  }
}
@media (max-width: 599px) {
  .header .logo__image2 {
    display: none;
  }
}
.header .logo__title {
  margin: auto 0 auto 0;
  font-family: "Sherlock";
  font-weight: lighter;
  font-size: 6.5rem;
  color: #d9d9d9;
  text-decoration: overline underline 3.5px;
}
@media (max-width: 599px) {
  .header .logo__title {
    font-size: 3em;
    text-decoration: overline underline 1.5px;
  }
}
@media (min-width: 600px) and (max-width: 899px) {
  .header .logo__title {
    text-align: center;
  }
}
.header .logo__menu {
  margin: auto;
  font-size: 3em;
  text-transform: lowercase;
}
@media (max-width: 599px) {
  .header .logo__menu {
    font-size: 1.7em;
  }
}
.header .logo__menu__list {
  display: flex;
  justify-content: center;
}
.header .logo__menu__list__item {
  padding: 15px;
}
.header .logo__menu__list__item a {
  text-decoration: none;
  color: #d9d9d9;
  font-family: "Germinabunt";
  transition: all 0.4s ease-in-out;
  background-image: linear-gradient(#BEA060, #1b0101);
  background-size: 0% 0.1em;
  background-position-y: 100%;
  background-position-x: 50%;
  background-repeat: no-repeat;
}
.header .logo__menu__list__item a:hover {
  background-size: 100% 0.1em;
  color: #BEA060;
}
.header .logo:hover {
  filter: invert(56%) sepia(49%) saturate(465%) hue-rotate(3deg) brightness(96%) contrast(84%);
}
.banner {
  width: 75%;
  margin: auto;
  display: flex;
  flex-direction: column;
  justify-content: center;
  text-align: center;
}
.banner__title {
  margin: 35px;
  font-size: 4em;
  text-transform: lowercase;
  letter-spacing: 2px;
  font-weight: lighter;
  font-family: "Germinabunt";
  color: #d9d9d9;
}
@media (max-width: 599px) {
  .banner__title {
    padding-top: 1em;
    font-size: 2.3em;
  }
}
.banner__front {
  margin: 35px;
  font-size: 5.5em;
  font-weight: lighter;
  font-family: "Germinabunt";
  color: #d9d9d9;
  letter-spacing: 2px;
  text-transform: lowercase;
}
@media (max-width: 599px) {
  .banner__front {
    padding-top: 1em;
    font-size: 3em;
  }
}
.banner__front__home {
  font-size: 1.5em;
  font-family: "Josefin-Medium";
  color: #d9d9d9;
  text-align: center;
}
@media (max-width: 599px) {
  .banner__front__home {
    font-size: 1.2em;
  }
}
.banner__p {
  margin-bottom: 25px;
  font-size: 1.5em;
  font-family: "Germinabunt";
  color: #d9d9d9;
}
.gallery {
  width: 75%;
  margin: auto;
}
@media (max-width: 599px) {
  .gallery {
    width: 95%;
  }
}
@media (min-width: 600px) and (max-width: 899px) {
  .gallery {
    width: 95%;
  }
}
@media (min-width: 900px) {
  .gallery {
    width: 95%;
  }
}
.gallery .flashs {
  grid-template-rows: repeat(12, 5vw);
}
@media (max-width: 599px) {
  .gallery .flashs {
    grid-template-rows: repeat(24, 8vw);
  }
}
.gallery__container {
  display: grid;
  grid-template-columns: repeat(12, 1fr);
  grid-template-rows: repeat(24, 5vw);
  grid-gap: 15px;
}
@media (max-width: 599px) {
  .gallery__container {
    grid-template-rows: repeat(45, 8vw);
    grid-gap: 5px;
  }
}
.gallery__container__item {
  width: auto;
}
.gallery__container__item img {
  width: 100%;
  height: 100%;
  object-fit: cover;
  border-radius: 5px;
  filter: grayscale(1);
  transition: all 1s ease-in-out;
}
.gallery__container__item img:hover {
  filter: grayscale(0.4);
  transition: all 0.4s ease-in-out;
  cursor: pointer;
}
.gallery__container .modal {
  margin: auto;
  display: none;
  position: fixed;
  z-index: 1;
  left: 0;
  top: 0;
  width: 100%;
  height: 100vh;
  overflow: auto;
  background-color: rgba(0, 0, 0, 0.9);
}
.gallery__container .modal-content {
  margin: auto;
  padding: 50px 0;
  display: block;
  width: 80%;
  max-width: 600px;
  animation-name: zoom;
  animation-duration: 0.6s;
}
@keyframes zoom {
  from {
    transform: scale(0);
  }
  to {
    transform: scale(1);
  }
}
.gallery__container .close {
  position: fixed;
  top: 15px;
  right: 35px;
  color: #f1f1f1;
  font-size: 40px;
  font-weight: bold;
  transition: 0.3s;
}
.gallery__container .close:hover, .gallery__container .close:focus {
  color: #bbb;
  text-decoration: none;
  cursor: pointer;
}
@media (max-width: 599px) {
  .gallery__container .close:hover, .gallery__container .close:focus {
    width: 100%;
  }
}
.gallery__container .item-0 {
  grid-column: 1/4;
  grid-row: 1/7;
}
